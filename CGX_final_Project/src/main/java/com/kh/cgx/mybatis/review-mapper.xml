<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="review">

	<insert id="insert" parameterType="reviewDto">
		insert into review VALUES(review_no_seq.nextval,#{ticket_no},#{review_egg},#{review_content},#{review_1},#{review_2},#{review_3},#{review_4},#{review_5})
	</insert>
</mapper>





